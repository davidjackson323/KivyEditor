<WidgetSelector@MDScrollView>
    id: add_content
    pos: self.parent.pos
    pos_hint: {'top':.95}
    #height: self.parent.height
    size_hint_y: 0.97
    size_hint_x: None
    width: '275dp'
    size_hint_max_x: 0.1
    bar_color: 0, 0, 1, 1
    bar_inactive_color: 0, 0, 0, 1

    bar_pos_y: 'left'

    canvas.before:
        Color:
            rgba: 0, 0, 0, 0.5

        Rectangle:
            pos:self.pos
            size: self.size



    MDBoxLayout:
        id: change_height
        md_bg_color: 0.1, 0.1, 0.1  , 0.1
        pos_hint: {'center_x': 0.5, 'top':0.5}
        size_hint_y: None
        height: self.parent.parent.height
        height: '1100dp'
        orientation: 'vertical'
        padding: '20dp'
        spacing: '15dp'
        orientation: 'vertical'



        MDLabel:
            text: 'Layouts'
            halign: 'center'
            font_size: '30sp'
            font_name: 'assets/fonts/racing.ttf'
            #background_color: 'black'
            color: 'white'
            size_hint_y: '50dp'
            



        MDRectangleFlatButton:
            size_hint_x: 0.9
            text: 'MD Float'
            pos_hint: {'center_x': 0.5, 'center_y':0.5}
            line_color: 'white'
            text_color: 'white'
            md_bg_color: 'black'
            on_release: root.manager.add_to()

